{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://tblend.io/protocol/riki/report-response.schema.json",
    "type": "object",
    "properties": {
        "summaryVC": {
            "type": "string",
            "description": "A Verifiable Credential JWT that provides a summary of the RIKI report"
        },
        "encryptedVC": {
            "type": "string",
            "description": "The full RIKI report JWT, encrypted"
        }
    },
    "required": [
        "summaryVC",
        "encryptedVC"
    ]
}